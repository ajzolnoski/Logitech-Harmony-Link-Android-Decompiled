<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:drawable="@drawable/icon_unlocked" />
    <item android:state_focused="true" android:state_checked="true">
        <layer-list>
            <item android:drawable="@drawable/icon_unlocked" />
            <item>
                <shape android:shape="rectangle"
                  xmlns:android="http://schemas.android.com/apk/res/android">
                    <solid android:color="#00000000" />
                    <stroke android:width="1.0dip" android:color="#ffffffff" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_focused="true" android:state_checked="false">
        <layer-list>
            <item android:drawable="@drawable/icon_locked" />
            <item>
                <shape android:shape="rectangle"
                  xmlns:android="http://schemas.android.com/apk/res/android">
                    <solid android:color="#00000000" />
                    <stroke android:width="1.0dip" android:color="#ffffffff" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:drawable="@drawable/icon_locked" />
</selector>